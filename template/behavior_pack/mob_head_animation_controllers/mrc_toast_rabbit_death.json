{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.mrc_toast_rabbit_death": {
			"states": {
				"default": {
					"transitions": [
						{
							"toast_dead": "query.is_name_any('Toast') && !query.is_alive && !query.is_baby"
						}
					]
				},
				"toast_dead": {
					"on_entry": [
						"/loot spawn ~ ~ ~ loot toast_rabbit"
					]
				}
			}
		}
	}
}